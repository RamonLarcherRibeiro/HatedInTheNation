ental e ambientalismo.</p>\n      <div id=\"bee2\" style=\"display: flex; align-items: center; justify-content: center;\">\n        <div ref=\"threeContainer2\" class=\"three-container2\"></div>\n      </div>\n      <div id=\"particles-js\" style=\"position: absolute; width: 80%; height: 100vh\"></div>\n    </div>\n  </div>\n</template>\n<script>\nimport * as THREE from 'three';\nimport ScrollReveal from 'scrollreveal';\nimport { ref } from 'vue';\nimport 'particles.js';\nimport { onMounted, nextTick } from 'vue';\n\nimport { OrbitControls } from 'three/examples/jsm/controls/OrbitControls.js';\nimport { PointLight } from 'three';\nimport { GLTFLoader } from 'three/examples/jsm/loaders/GLTFLoader.js';\n\nexport default {\n  setup() {\n    onMounted(() => {\n      nextTick(() => {\n        window.particlesJS('particles-js', {\n          \"particles\": {\n            \"number\": {\n              \"value\": 80,\n              \"density\": {\n                \"enable\": true,\n                \"value_area\": 800\n              }\n            },\n            \"color\": {\n              \"value\": \"#ffffff\"\n            },\n            \"shape\": {\n              \"type\": \"circle\",\n              \"stroke\": {\n                \"width\": 0,\n                \"color\": \"#000000\"\n              },\n              \"polygon\": {\n                \"nb_sides\": 5\n              },\n              \"image\": {\n                \"src\": \"img/github.svg\",\n                \"width\": 100,\n                \"height\": 100\n              }\n            },\n            \"opacity\": {\n              \"value\": 0.5,\n              \"random\": false,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 1,\n                \"opacity_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"size\": {\n              \"value\": 3,\n              \"random\": true,\n              \"anim\": {\n                \"enab