{"ast":null,"code":"import * as THREE from 'three';\nimport ScrollReveal from 'scrollreveal';\nimport 'particles.js';\nimport { onMounted, nextTick } from 'vue';\nimport { OrbitControls } from 'three/examples/jsm/controls/OrbitControls.js';\nimport { PointLight } from 'three';\nimport { GLTFLoader } from 'three/examples/jsm/loaders/GLTFLoader.js';\nexport default {\n  setup() {\n    onMounted(() => {\n      nextTick(() => {\n        window.particlesJS('particles-js', {\n          \"particles\": {\n            \"number\": {\n              \"value\": 80,\n              \"density\": {\n                \"enable\": true,\n                \"value_area\": 800\n              }\n            },\n            \"color\": {\n              \"value\": \"#ffffff\"\n            },\n            \"shape\": {\n              \"type\": \"circle\",\n              \"stroke\": {\n                \"width\": 0,\n                \"color\": \"#000000\"\n              },\n              \"polygon\": {\n                \"nb_sides\": 5\n              },\n              \"image\": {\n                \"src\": \"img/github.svg\",\n                \"width\": 100,\n                \"height\": 100\n              }\n            },\n            \"opacity\": {\n              \"value\": 0.5,\n              \"random\": false,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 1,\n                \"opacity_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"size\": {\n              \"value\": 3,\n              \"random\": true,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 40,\n                \"size_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"line_linked\": {\n              \"enable\": true,\n              \"distance\": 150,\n              \"color\": \"#ffffff\",\n              \"opacity\": 0.4,\n              \"width\": 1\n            },\n            \"move\": {\n              \"enable\": true,\n              \"speed\": 6,\n              \"direction\": \"none\",\n              \"random\": false,\n              \"straight\": false,\n              \"out_mode\": \"out\",\n              \"bounce\": false,\n              \"attract\": {\n                \"enable\": false,\n                \"rotateX\": 600,\n                \"rotateY\": 1200\n              }\n            }\n          },\n          \"interactivity\": {\n            \"detect_on\": \"canvas\",\n            \"events\": {\n              \"onhover\": {\n                \"enable\": true,\n                \"mode\": \"repulse\"\n              },\n              \"onclick\": {\n                \"enable\": true,\n                \"mode\": \"push\"\n              },\n              \"resize\": true\n            },\n            \"modes\": {\n              \"grab\": {\n                \"distance\": 400,\n                \"line_linked\": {\n                  \"opacity\": 1\n                }\n              },\n              \"bubble\": {\n                \"distance\": 400,\n                \"size\": 40,\n                \"duration\": 2,\n                \"opacity\": 8,\n                \"speed\": 3\n              },\n              \"repulse\": {\n                \"distance\": 200,\n                \"duration\": 0.4\n              },\n              \"push\": {\n                \"particles_nb\": 4\n              },\n              \"remove\": {\n                \"particles_nb\": 2\n              }\n            }\n          },\n          \"retina_detect\": true\n        });\n        window.particlesJS('particles-js2', {\n          \"particles\": {\n            \"number\": {\n              \"value\": 80,\n              \"density\": {\n                \"enable\": true,\n                \"value_area\": 800\n              }\n            },\n            \"color\": {\n              \"value\": \"#ffffff\"\n            },\n            \"shape\": {\n              \"type\": \"circle\",\n              \"stroke\": {\n                \"width\": 0,\n                \"color\": \"#000000\"\n              },\n              \"polygon\": {\n                \"nb_sides\": 5\n              },\n              \"image\": {\n                \"src\": \"img/github.svg\",\n                \"width\": 100,\n                \"height\": 100\n              }\n            },\n            \"opacity\": {\n              \"value\": 0.5,\n              \"random\": false,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 1,\n                \"opacity_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"size\": {\n              \"value\": 3,\n              \"random\": true,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 40,\n                \"size_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"line_linked\": {\n              \"enable\": true,\n              \"distance\": 150,\n              \"color\": \"#ffffff\",\n              \"opacity\": 0.4,\n              \"width\": 1\n            },\n            \"move\": {\n              \"enable\": true,\n              \"speed\": 6,\n              \"direction\": \"none\",\n              \"random\": false,\n              \"straight\": false,\n              \"out_mode\": \"out\",\n              \"bounce\": false,\n              \"attract\": {\n                \"enable\": false,\n                \"rotateX\": 600,\n                \"rotateY\": 1200\n              }\n            }\n          },\n          \"interactivity\": {\n            \"detect_on\": \"canvas\",\n            \"events\": {\n              \"onhover\": {\n                \"enable\": true,\n                \"mode\": \"repulse\"\n              },\n              \"onclick\": {\n                \"enable\": true,\n                \"mode\": \"push\"\n              },\n              \"resize\": true\n            },\n            \"modes\": {\n              \"grab\": {\n                \"distance\": 400,\n                \"line_linked\": {\n                  \"opacity\": 1\n                }\n              },\n              \"bubble\": {\n                \"distance\": 400,\n                \"size\": 40,\n                \"duration\": 2,\n                \"opacity\": 8,\n                \"speed\": 3\n              },\n              \"repulse\": {\n                \"distance\": 200,\n                \"duration\": 0.4\n              },\n              \"push\": {\n                \"particles_nb\": 4\n              },\n              \"remove\": {\n                \"particles_nb\": 2\n              }\n            }\n          },\n          \"retina_detect\": true\n        });\n      });\n    });\n  },\n  name: 'ThreeComponent',\n  mounted() {\n    const sr = ScrollReveal();\n    const container = this.$refs.threeContainer;\n    if (!container) {\n      console.error('Element with ref \"threeContainer\" not found');\n      return;\n    }\n    const renderer = new THREE.WebGLRenderer({\n      alpha: true\n    });\n    renderer.setSize(200, 200);\n    container.appendChild(renderer.domElement);\n    const scene = new THREE.Scene();\n    scene.background = null;\n    const camera = new THREE.PerspectiveCamera(45, 1, 1, 3000);\n    camera.rotation.y = Math.PI;\n    camera.position.x = 800;\n    camera.position.y = 100;\n    camera.position.z = 1000;\n    const controls = new OrbitControls(camera, renderer.domElement);\n    controls.update();\n    const light1 = new PointLight(0xc4c4c4, 500000);\n    light1.position.set(0, 300, 500);\n    scene.add(light1);\n    const light2 = new PointLight(0xc4c4c4, 500000);\n    light2.position.set(500, 100, 0);\n    scene.add(light2);\n    const light3 = new PointLight(0xc4c4c4, 500000);\n    light3.position.set(0, 100, -500);\n    scene.add(light3);\n    const light4 = new PointLight(0xc4c4c4, 50000);\n    light4.position.set(-500, 300, 500);\n    scene.add(light4);\n    const gltfLoader = new GLTFLoader();\n    gltfLoader.load('/assets/bee_gltf.glb', gltf => {\n      const model = gltf.scene;\n      model.scale.set(600, 600, 600);\n      model.position.y = -50;\n      scene.add(model);\n      if (gltf.animations && gltf.animations.length > 0) {\n        const mixer = new THREE.AnimationMixer(model);\n        gltf.animations.forEach(clip => {\n          mixer.clipAction(clip).play();\n        });\n        const animate = () => {\n          requestAnimationFrame(animate);\n          controls.update();\n          mixer.update(0.01);\n          renderer.render(scene, camera);\n        };\n        animate();\n      } else {\n        console.log('Nenhuma animação encontrada no modelo.');\n      }\n    });\n\n    //BEE2\n    const container2 = this.$refs.threeContainer2;\n    const renderer2 = new THREE.WebGLRenderer({\n      alpha: true\n    });\n    renderer2.setSize(330, 300);\n    container2.appendChild(renderer2.domElement);\n    const scene2 = new THREE.Scene();\n    scene.background = null;\n    const camera2 = new THREE.PerspectiveCamera(45, 1, 1, 3000);\n    camera2.rotation.y = Math.PI;\n    camera2.position.x = -1700;\n    camera2.position.y = 500;\n    camera2.position.z = 1000;\n    const controls2 = new OrbitControls(camera2, renderer2.domElement);\n    controls2.update();\n    const bee2light1 = new PointLight(0xc4c4c4, 800000);\n    bee2light1.position.set(0, 300, 500);\n    scene2.add(bee2light1);\n    const bee2light2 = new PointLight(0xc4c4c4, 800000);\n    bee2light2.position.set(500, 100, 0);\n    scene2.add(bee2light2);\n    const bee2light3 = new PointLight(0xc4c4c4, 800000);\n    bee2light3.position.set(0, 100, -500);\n    scene2.add(bee2light3);\n    const bee2light4 = new PointLight(0xc4c4c4, 80000);\n    bee2light4.position.set(-500, 300, 500);\n    scene2.add(bee2light4);\n    const gltfLoader2 = new GLTFLoader();\n    gltfLoader2.load('/assets/bee1.glb', gltf => {\n      const model = gltf.scene;\n      model.scale.set(300, 300, 300);\n      model.position.y = -50;\n      scene2.add(model);\n      if (gltf.animations && gltf.animations.length > 0) {\n        const mixer = new THREE.AnimationMixer(model);\n        gltf.animations.forEach(clip => {\n          mixer.clipAction(clip).play();\n        });\n        const animate = () => {\n          requestAnimationFrame(animate);\n          controls2.update();\n          mixer.update(0.01);\n          renderer2.render(scene2, camera2);\n        };\n        animate();\n      } else {\n        console.log('Nenhuma animação encontrada no modelo.');\n      }\n    });\n    const animateSpan = id => {\n      const span = document.querySelector(\"#\" + id);\n      //texto original sem modificações \n      const textSpan = span.textContent;\n      if (id == 'hated') {\n        const typesSpan = ['Hated', 'H ted', 'Ha ed', 'Hat d'];\n        let indiceSorteado = Math.floor(Math.random() * typesSpan.length);\n        let elementoSorteado = typesSpan[indiceSorteado];\n        span.textContent = elementoSorteado + ' ';\n        span.style.setProperty('text-shadow', 'rgb(255, 255, 53) 0px 0px 8px', 'important');\n      } else if (id == 'nation') {\n        const typesSpan = ['N tion', 'Na ion', 'Nat on', 'Nati n'];\n        let indiceSorteado = Math.floor(Math.random() * typesSpan.length);\n        let elementoSorteado = typesSpan[indiceSorteado];\n        span.textContent = elementoSorteado + ' ';\n        span.style.setProperty('text-shadow', 'rgb(255, 255, 53) 0px 0px 8px', 'important');\n      }\n      //voltar com o texto original\n      setTimeout(() => {\n        span.textContent = textSpan + \" \";\n        span.style.setProperty('text-shadow', 'rgb(255, 255, 53) 0px 0px 13px', 'important');\n      }, 300);\n    };\n    const animateNeon = () => {\n      //array de spans (palavras) que serao animados(as)\n      const spans = ['hated', 'nation'];\n      setInterval(() => {\n        //sortear spans que serao animados\n        let indiceSorteado = Math.floor(Math.random() * spans.length);\n        let elementoSorteado = spans[indiceSorteado];\n        animateSpan(elementoSorteado);\n      }, 8000);\n    };\n    animateNeon();\n    sr.reveal('#div-topo', {\n      duration: 8000,\n      dalay: 800,\n      origin: 'left',\n      distance: '1000px',\n      easing: 'ease-out',\n      mobile: true\n    });\n    sr.reveal('#bee1', {\n      duration: 20000,\n      dalay: 1000,\n      origin: 'top',\n      distance: '1000px',\n      easing: 'ease-out',\n      mobile: true\n    });\n    sr.reveal('#texto', {\n      duration: 5000,\n      dalay: 500,\n      origin: 'left',\n      distance: '800px',\n      easing: 'ease-out',\n      mobile: true\n    });\n    sr.reveal('#bee2', {\n      duration: 15000,\n      dalay: 4000,\n      origin: 'right',\n      easing: 'ease-out',\n      mobile: true\n    });\n  }\n};","map":{"version":3,"names":["THREE","ScrollReveal","onMounted","nextTick","OrbitControls","PointLight","GLTFLoader","setup","window","particlesJS","name","mounted","sr","container","$refs","threeContainer","console","error","renderer","WebGLRenderer","alpha","setSize","appendChild","domElement","scene","Scene","background","camera","PerspectiveCamera","rotation","y","Math","PI","position","x","z","controls","update","light1","set","add","light2","light3","light4","gltfLoader","load","gltf","model","scale","animations","length","mixer","AnimationMixer","forEach","clip","clipAction","play","animate","requestAnimationFrame","render","log","container2","threeContainer2","renderer2","scene2","camera2","controls2","bee2light1","bee2light2","bee2light3","bee2light4","gltfLoader2","animateSpan","id","span","document","querySelector","textSpan","textContent","typesSpan","indiceSorteado","floor","random","elementoSorteado","style","setProperty","setTimeout","animateNeon","spans","setInterval","reveal","duration","dalay","origin","distance","easing","mobile"],"sources":["/home/ramon/repositorios/hatedinthenation/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"container\" style=\"background-color: black;\">\n    <div id=\"contact\" style=\"z-index:15 ;width: 100%; display: flex; justify-content: end;\">\n      <a href=\"https://www.linkedin.com/in/ramon-larcher-b50806270/\" style=\"font-family: monospace;transition: text-shadow 0.3s ease;text-decoration: none ;font-size: 20px ; padding-right:10px ;color: white; text-shadow:0px 0px 3px rgba(255, 255, 53, 1); \">Contact</a>\n    </div>\n    <div id=\"particles-js2\" style=\"z-index: 6; position: absolute; width: 100%; height: 100vh\"></div>\n\n    <div id=\"div-topo\" style=\"display: flex; flex-direction: row; align-items: center;justify-content: center;\"> \n      <h1 id=\"titulo\">\n        <span id=\"hated\">Hated </span>\n        <span id=\"in\">in </span>\n        <span id=\"the\">the </span>\n        <span id=\"nation\">Nation </span>\n      </h1>\n      <div id=\"bee1\">\n        <div ref=\"threeContainer\" class=\"three-container\"></div>\n      </div>\n    </div>\n\n    <div id=\"first-text\" style=\"display: flex; flex-direction: column; height: 100vh; align-items: center; justify-content:center; \">\n      <p id=\"texto\" style=\"color:white;\">Hated in the Nation \" é o sexto e último episódio da terceira temporada da série antológica britânica de ficção científica Black Mirror . Escrito pelo criador e showrunner da série Charlie Brooker e dirigido por James Hawes , estreou na Netflix em 21 de outubro de 2016, junto com o restante da terceira série. É o episódio mais longo de Black Mirror , com 89 minutos.Um episódio de inspiração nórdica noir ,<span style=\"background-color:rgb(157 157 39); color: black; \"> \"Hated in the Nation\" segue os detetives Karin Parke ( Kelly Macdonald ) e Blue Coulson ( Faye Marsay ) enquanto eles investigam uma série de mortes visando os sujeitos do ódio nas redes sociais , nas mãos de Insetos Drones Autônomos</span>. (ADIs) que foram implantadas para combater catástrofes ambientais à medida que as abelhas estão próximas da extinção. Foi filmado principalmente em Londres.Baseado na experiência de Brooker ao receber cartas de ódio depois de escrever uma coluna no Guardian em 2004 que menosprezava George W. Bush , o episódio foi comparado com Arquivo X e explorou temas como vigilância governamental e ambientalismo.</p>\n      <div id=\"bee2\" style=\"display: flex; align-items: center; justify-content: center; margin-top: 10px; z-index: 10;\">\n        <div ref=\"threeContainer2\" class=\"three-container2\"></div>\n      </div>\n      <div id=\"particles-js\" style=\"position: absolute; width: 100%; height: 100vh; z-index: 0;\"></div>\n\n    </div>\n\n\n  </div>\n</template>\n<script>\nimport * as THREE from 'three';\nimport ScrollReveal from 'scrollreveal';\nimport 'particles.js';\nimport { onMounted, nextTick } from 'vue';\n\nimport { OrbitControls } from 'three/examples/jsm/controls/OrbitControls.js';\nimport { PointLight } from 'three';\nimport { GLTFLoader } from 'three/examples/jsm/loaders/GLTFLoader.js';\n\nexport default {\n  setup() {\n    onMounted(() => {\n      nextTick(() => {\n        window.particlesJS('particles-js',{\n          \"particles\": {\n            \"number\": {\n              \"value\": 80,\n              \"density\": {\n                \"enable\": true,\n                \"value_area\": 800\n              }\n            },\n            \"color\": {\n              \"value\": \"#ffffff\"\n            },\n            \"shape\": {\n              \"type\": \"circle\",\n              \"stroke\": {\n                \"width\": 0,\n                \"color\": \"#000000\"\n              },\n              \"polygon\": {\n                \"nb_sides\": 5\n              },\n              \"image\": {\n                \"src\": \"img/github.svg\",\n                \"width\": 100,\n                \"height\": 100\n              }\n            },\n            \"opacity\": {\n              \"value\": 0.5,\n              \"random\": false,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 1,\n                \"opacity_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"size\": {\n              \"value\": 3,\n              \"random\": true,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 40,\n                \"size_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"line_linked\": {\n              \"enable\": true,\n              \"distance\": 150,\n              \"color\": \"#ffffff\",\n              \"opacity\": 0.4,\n              \"width\": 1\n            },\n            \"move\": {\n              \"enable\": true,\n              \"speed\": 6,\n              \"direction\": \"none\",\n              \"random\": false,\n              \"straight\": false,\n              \"out_mode\": \"out\",\n              \"bounce\": false,\n              \"attract\": {\n                \"enable\": false,\n                \"rotateX\": 600,\n                \"rotateY\": 1200\n              }\n            }\n          },\n          \"interactivity\": {\n            \"detect_on\": \"canvas\",\n            \"events\": {\n              \"onhover\": {\n                \"enable\": true,\n                \"mode\": \"repulse\"\n              },\n              \"onclick\": {\n                \"enable\": true,\n                \"mode\": \"push\"\n              },\n              \"resize\": true\n            },\n            \"modes\": {\n              \"grab\": {\n                \"distance\": 400,\n                \"line_linked\": {\n                  \"opacity\": 1\n                }\n              },\n              \"bubble\": {\n                \"distance\": 400,\n                \"size\": 40,\n                \"duration\": 2,\n                \"opacity\": 8,\n                \"speed\": 3\n              },\n              \"repulse\": {\n                \"distance\": 200,\n                \"duration\": 0.4\n              },\n              \"push\": {\n                \"particles_nb\": 4\n              },\n              \"remove\": {\n                \"particles_nb\": 2\n              }\n            }\n          },\n          \"retina_detect\": true\n        });\n\n        window.particlesJS('particles-js2',{\n          \"particles\": {\n            \"number\": {\n              \"value\": 80,\n              \"density\": {\n                \"enable\": true,\n                \"value_area\": 800\n              }\n            },\n            \"color\": {\n              \"value\": \"#ffffff\"\n            },\n            \"shape\": {\n              \"type\": \"circle\",\n              \"stroke\": {\n                \"width\": 0,\n                \"color\": \"#000000\"\n              },\n              \"polygon\": {\n                \"nb_sides\": 5\n              },\n              \"image\": {\n                \"src\": \"img/github.svg\",\n                \"width\": 100,\n                \"height\": 100\n              }\n            },\n            \"opacity\": {\n              \"value\": 0.5,\n              \"random\": false,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 1,\n                \"opacity_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"size\": {\n              \"value\": 3,\n              \"random\": true,\n              \"anim\": {\n                \"enable\": false,\n                \"speed\": 40,\n                \"size_min\": 0.1,\n                \"sync\": false\n              }\n            },\n            \"line_linked\": {\n              \"enable\": true,\n              \"distance\": 150,\n              \"color\": \"#ffffff\",\n              \"opacity\": 0.4,\n              \"width\": 1\n            },\n            \"move\": {\n              \"enable\": true,\n              \"speed\": 6,\n              \"direction\": \"none\",\n              \"random\": false,\n              \"straight\": false,\n              \"out_mode\": \"out\",\n              \"bounce\": false,\n              \"attract\": {\n                \"enable\": false,\n                \"rotateX\": 600,\n                \"rotateY\": 1200\n              }\n            }\n          },\n          \"interactivity\": {\n            \"detect_on\": \"canvas\",\n            \"events\": {\n              \"onhover\": {\n                \"enable\": true,\n                \"mode\": \"repulse\"\n              },\n              \"onclick\": {\n                \"enable\": true,\n                \"mode\": \"push\"\n              },\n              \"resize\": true\n            },\n            \"modes\": {\n              \"grab\": {\n                \"distance\": 400,\n                \"line_linked\": {\n                  \"opacity\": 1\n                }\n              },\n              \"bubble\": {\n                \"distance\": 400,\n                \"size\": 40,\n                \"duration\": 2,\n                \"opacity\": 8,\n                \"speed\": 3\n              },\n              \"repulse\": {\n                \"distance\": 200,\n                \"duration\": 0.4\n              },\n              \"push\": {\n                \"particles_nb\": 4\n              },\n              \"remove\": {\n                \"particles_nb\": 2\n              }\n            }\n          },\n          \"retina_detect\": true\n        });\n      \n      });\n    });\n  \n  },\n  name: 'ThreeComponent',\n  mounted() {\n   \n    const sr = ScrollReveal();\n\n    const container = this.$refs.threeContainer;\n    if (!container) {\n      console.error('Element with ref \"threeContainer\" not found');\n      return;\n    }\n    const renderer = new THREE.WebGLRenderer({ alpha: true });\n\n    renderer.setSize(200, 200);\n    container.appendChild(renderer.domElement);\n\n    const scene = new THREE